<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.akash.fbla_library">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CreateAccount"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UserPage"
            android:label="@string/title_activity_user_page"
            android:screenOrientation="portrait" />
        <activity
            android:name=".UserAccountPage"
            android:screenOrientation="portrait" />
        <activity
            android:name=".BookAttributeDisplay"
            android:parentActivityName=".UserPage"
            android:screenOrientation="portrait" />
        <activity android:name=".BookAttributeActivity" />
        <activity android:name=".CheckoutActivity" />
        <activity android:name=".HoldActivity" />
        <activity android:name=".BookHoldQueueActivity" />
        <activity android:name=".FeedbackActivity" />
        <activity android:name=".BookMarkActivity" />
        <activity
            android:name=".ProfileEditPage"
            android:label="@string/title_activity_profile_edit_page" />
        <activity android:name=".AboutActivity" />
        <activity android:name=".DuesActivity"></activity>
    </application>

</manifest>